pcm.rawjack {
    type jack
     playback_ports {
        0 gstreamer-send-01:in_jackaudiosrc0_1
        1 gstreamer-send-01:in_jackaudiosrc0_2
     }

    capture_ports {
        0 gstreamer-send:out_jackaudiosink0_1
        1 gstreamer-send:out_jackaudiosink0_2
     }
}
pcm.jack {
    type plug
    slave { pcm "rawjack" }
    hint {
        description "JACK Audio Connection Kit"
    }
}
pcm.!default {
    type plug
    slave { pcm "rawjack" }
}
